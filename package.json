{
  "name": "vimeo_volumetric_video",
  "version": "0.0.1",
  "description": "A volumetric video player for playing back color-depth videos hosted on Vimeo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"watchify src/index.js -o dist/vimeo.depth-player.js -v\"",
    "build": "browserify src/index.js | uglifyjs --source-map --output dist/vimeo.depth-player.min.js",
    "test": "npm run build"
  },
  "author": "Or Fleisher <or@vimeo.com>",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.4",
    "ejs": "^2.6.1",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.4.0",
    "path": "^0.12.7",
    "sync-request": "^6.0.0",
    "three": "^0.89.0",
    "vimeo": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "concurrently": "^3.5.1",
    "dat.gui": "^0.7.2",
    "dotenv": "^6.0.0",
    "event-emitter-es6": "^1.1.5",
    "glslify": "^6.1.0",
    "nodemon": "^1.14.12",
    "uglify-js": "^3.3.9",
    "watchify": "^3.10.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ],
      "glslify"
    ]
  }
}
